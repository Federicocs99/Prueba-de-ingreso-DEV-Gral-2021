@model EnvioApi
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link href="~/css/style.css" rel="stylesheet" type="text/css">
    <title>NewShore_</title>
</head>

<body>
  <div asp-validarion-summary="All"></div>
  <form method="Post">
  <div class="fly-date">

      <div class="col-md-3">
        <label for="validationDefault04" class="form-label">Destino</label>
        <input asp-for="Origin" /> 
      </div>

      <div class="col-md-3">
        <label for="validationDefault04" class="form-label">Destino</label>
        <input asp-for="Destination" /> 
      </div>
  
      <div class="form-group">
      <input type="date" id="start" name="trip-start" 
             aps-for="From" Format = "dd/mm/yyyy"
             value="2021-10-16"
             min="2021-10-15" max="2022-12-31">
      </div> 

      <script src="index.js" type="module"></script>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    </div>
     <div class="cta">
     <input type="submit">
     </div> 
  </form>
</body>
</html>